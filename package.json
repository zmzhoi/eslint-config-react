{
  "name": "@zmzhoi/eslint-config-react",
  "version": "1.0.2",
  "description": "This package is a eslint sharable config for React project.",
  "main": "index.js",
  "keywords": [
    "eslint",
    "shareable eslint config",
    "eslintconfig",
    "react",
    "create-react-app"
  ],
  "files": [
    "index.js",
    "utils.js"
  ],
  "author": "zmzhoi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zmzhoi/eslint-config-react"
  },
  "homepage": "https://github.com/zmzhoi/eslint-config-react",
  "scripts": {
    "format": "prettier --check \"**/*.(js|json|mjs|cjs|md)\"",
    "format:fix": "prettier --write \"**/*.(js|json|mjs|cjs|md)\"",
    "publish:pre": "npm version prerelease && git push origin master --tags",
    "publish:patch": "npm version patch && git push origin master --tags",
    "publish:minor": "npm version minor && git push origin master --tags",
    "publish:major": "npm version major && git push origin master --tags"
  },
  "dependencies": {
    "@babel/core": "^7.18.10",
    "@babel/eslint-parser": "^7.18.9",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0"
  },
  "peerDependencies": {
    "eslint": "^8.0.0"
  },
  "devDependencies": {
    "eslint": "^8.22.0",
    "prettier": "^2.7.1"
  }
}
